<body>
<table width="80%" align="center" cellspacing="0" cellpadding="10" border="1">
    <tr>
        <td colspan="2" valign="middle" height="70">  
			<table width="100%">
                <tr>
                    <td>
                            <img height="48" src="/resource/logo.png">
                    </td>
                    <td align="right">
                        Logged in as <a href="/adminhome" target="iFrame"><%=name%></a>&nbsp;|
                        <a href="/logout">Logout</a>
                    </td>
                </tr>
			</table>
        </td>
    </tr>
    <tr>
	<td width="190" valign="top">
			<b>&nbsp;Admin Panel</b><hr/>
            <ul>
                <li><a href="/adminhome" target="iFrame">Dashboard</a></li>
                <li><a href="/adminhome/student" target="iFrame">Students Info</a></li>
                <li><a href="/adminhome/teacher" target="iFrame">Teachers Info</a></li>
				<li><a href="/adminhome/search" target="iFrame">Search User</a></li>
                <li><a href="/adminhome/subject" target="iFrame">Add New Subject</a></li>
				<li><a href="/adminhome/assignteacher" target="iFrame">Assign Teacher To Subject</a></li>
				<li><a href="/adminhome/sendnotice" target="iFrame">Send Notice</a></li>
				<li><a href="/adminhome/studentreport" target="iFrame">Generate Student Report</a></li>
            </ul> 
			<b>&nbsp;Personal</b><hr/>
            <ul>
                <li><a href="/logout">Logout</a></li>
            </ul>
        </td>
        <td valign="top">

		
		
		
		
		
		
	<fieldset>
    <legend><b>Assign Teacher To Subject</b></legend>
	<form method="POST">
		<br/>
		<table width="100%" cellpadding="0" cellspacing="0">
			<tr>
				<td>Class</td>
				<td>:</td>
				<td>
					<select name = "cls">
					<option value="1">1</option>
					<option value="2">2</option>
					<option value="3">3</option>
					<option value="4">4</option>
					<option value="5">5</option>
					<option value="6">6</option>
					<option value="7">7</option>
					<option value="8">8</option>
					<option value="9">9</option>
					<option value="10">10</option>
					</select>
				</td>
				<td></td>
			</tr>
			<tr><td colspan="4"><hr/></td></tr>
		</table>
		<center><input type="submit" value="Select" name="submit"></center>
		<hr/>
		</form>
		<form method="POST">
		<br/>
		<table width="100%" cellpadding="0" cellspacing="0">
			<tr>
				<td>Subject Name</td>
				<td>:</td>
				<td>
					<select name = "subjectid">
					<% for(var i=0; i < uList.length; i++){ %>
					<option value="<%= uList[i].id %>"><%= uList[i].subjectname %></option>
					<% } %>
					</select>
				</td>
				<td></td>
			</tr>
			<tr><td colspan="4"><hr/></td></tr>
			<tr>
				<td>Teacher Name</td>
				<td>:</td>
				<td>
					<select name = "teachername">
					<% for(var i=0; i < tList.length; i++){ %>
					<option value="<%= tList[i].name %>"><%= tList[i].name %></option>
					<% } %>
					</select>
				</td>
				<td></td>
			</tr>
			<tr><td colspan="4"><hr/></td></tr>
		</table>
		<center><input type="submit" value="Submit" name="submit"><center>
		<hr/>
	</form>
</fieldset>
	
	

	
	</td>
    </tr>
    <tr>
        <td colspan="2" align="center">
